{
  "name": "embrace.mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "expo start --dev-client",
    "prebuild": "pnpm expo prebuild",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "doctor": "npx expo-doctor",
    "preinstall": "npx only-allow pnpm",
    "start:staging": "APP_ENV=staging expo start --dev-client",
    "prebuild:staging": "APP_ENV=staging pnpm expo prebuild",
    "android:staging": "APP_ENV=staging expo run:android",
    "ios:staging": "APP_ENV=staging expo run:ios",
    "start:production": "APP_ENV=production expo start --dev-client",
    "prebuild:production": "APP_ENV=production pnpm expo prebuild",
    "android:production": "APP_ENV=production expo run:android",
    "ios:production": "APP_ENV=production expo run:ios",
    "build:development:ios": "APP_ENV=development eas build --profile development --platform ios",
    "build:development:android": "APP_ENV=development eas build --profile development --platform android ",
    "build:staging:ios": "APP_ENV=staging eas build --profile staging --platform ios",
    "build:staging:android": "APP_ENV=staging eas build --profile staging --platform android ",
    "build:production:ios": "APP_ENV=production eas build --profile production --platform ios",
    "build:production:android": "APP_ENV=production eas build --profile production --platform android ",
    "postinstall": "husky install",
    "np-release": "SKIP_BRANCH_PROTECTION=true np --no-publish --no-cleanup",
    "version": "pnpm run prebuild && git add .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc  --noemit",
    "lint:translations": "eslint ./src/translations/ --fix --ext .json  ",
    "test": "jest",
    "test:watch": "pnpm run test --watch",
    "test:ci": "pnpm run test --coverage",
    "test:badges": "pnpm test:ci  && jest-coverage-badges --input coverage/coverage-summary.json --output __badges__"
  },
  "dependencies": {
    "@bacons/link-assets": "^1.1.0",
    "@bundlr-network/client": "^0.11.9",
    "@ethersproject/shims": "^5.7.0",
    "@gorhom/bottom-sheet": "^4.4.7",
    "@hookform/resolvers": "^2.9.11",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@realm/react": "^0.5.1",
    "@shopify/flash-list": "1.4.0",
    "@tanstack/react-query": "^4.29.14",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "ethers": "^6.6.0",
    "expo": "~49.0.4",
    "expo-constants": "~14.4.2",
    "expo-dev-client": "~2.4.6",
    "expo-file-system": "~15.2.2",
    "expo-image": "~1.3.2",
    "expo-image-manipulator": "~11.1.1",
    "expo-image-picker": "~14.1.1",
    "expo-localization": "~14.3.0",
    "expo-splash-screen": "~0.20.4",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-updates": "~0.16.4",
    "i18next": "^22.5.1",
    "lodash.memoize": "^4.1.2",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.45.0",
    "react-i18next": "^12.3.1",
    "react-native": "0.72.3",
    "react-native-flash-message": "^0.4.1",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-mmkv": "2.6.3",
    "react-native-quick-base64": "^2.0.6",
    "react-native-quick-crypto": "^0.6.1",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-svg": "13.4.0",
    "react-native-web": "~0.18.12",
    "react-query-kit": "^1.4.5",
    "realm": "^11.10.1",
    "zod": "^3.21.4",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-proposal-decorators": "^7.22.5",
    "@babel/plugin-transform-flow-strip-types": "^7.22.5",
    "@babel/plugin-transform-private-methods": "^7.22.5",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@expo/config": "~8.0.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@realm/babel-plugin": "^0.1.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^11.5.4",
    "@types/i18n-js": "^3.8.4",
    "@types/jest": "^29.5.2",
    "@types/lodash.memoize": "^4.1.7",
    "@types/react": "~18.0.38",
    "@types/react-native": "~0.71.8",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.43.0",
    "eslint-plugin-i18n-json": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "eslint-plugin-unicorn": "^46.0.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-jsdom": "^29.5.0",
    "jest-expo": "48.0.1",
    "lint-staged": "^13.2.2",
    "metro-babel-register": "^0.73.10",
    "np": "^7.7.0",
    "prettier": "^2.8.8",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5"
  },
  "osMetadata": {
    "initVersion": "3.3.1"
  }
}
